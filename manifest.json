{
  "version": "0.3.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "name": "Dark Shogun",
  "description": "Admin dark mode",
  "author": "Jill @ Shogun",
  "content_scripts": [
    {
      "css": ["darkmode.min.css"],
      "matches": [
        "https://getshogun.com/admin*",
        "https://next-admin.getshogun.com/*",
        "https://admin.getshogun.com/*",
        "https://app.intercom.io/*",
        "https://app.intercom.com/*"
      ]
    }
  ]
}
